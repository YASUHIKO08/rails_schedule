<section class = new_record>
  <nav>
    <h3>予定の編集</h3>
  </nav>
  <div class = table_all>
    <%= form_with model: @user do |f| %>
      <table class = table_record>
        <th><%= f.label :title,"タイトル" %></th>
        <td><%= f.text_field :title %></td>
      </table>
      <table class = table_record>
        <th><%= f.label :start,"開始日" %></th>
        <td><%= f.text_field :start %></td>
        <th><%= f.label :end,"終了日" %></th>
        <td><%= f.text_field :end %></td>
      </table>
      <table class = table_record>
        <tr>
        <p><%= f.label :all_day,"終日" %></p>
        <p><%= f.check_box :all_day, checked: true, unchecked_value: false %></p>
        <th><%= f.label :memo,"スケジュールメモ" %></th>
        <td><%= f.text_area:memo %></td>
      </tr>
      </table>
      <p><%= f.submit "更新" %></p>
      <p><%= link_to "キャンセル", :users %></p>
    <% end %>
  </div>
</section>